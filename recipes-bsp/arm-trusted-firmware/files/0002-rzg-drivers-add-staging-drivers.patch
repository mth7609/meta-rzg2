+static uint32_t _board_judge(void)
+{
+	uint32_t brd;
+
+	if (prr_product == PRR_PRODUCT_G2H) {
+		brd = 4;
+	} else if (prr_product == PRR_PRODUCT_G2M) {
+		if (prr_cut <= RZG_G2M_CUT_VER11) {
+			brd = 1;
+		} else {
+			uint32_t lpddr4_2rank = 0x01 << 25;
+			if (mmio_read_32(GPIO_INDT5) & lpddr4_2rank) {
+				brd = 1;
+			} else {
+				brd = 0;
+			}
+		}
+	} else {
+		brd = 2;
+	}
+
+	return brd;
+}
